{
    "name": "cw.u2re.space",
    "short_name": "CW-i1",
    "description": "AI-and-control-toolset",
    "start_url": "/",
    "scope": "/",
    "display": "standalone",
    "display_override": [
        "window-controls-overlay",
        "standalone"
    ],
    "background_color": "#000000",
    "theme_color": "#000000",
    "categories": [
        "utilities",
        "productivity",
        "lifestyle",
        "finance",
        "navigation",
        "education"
    ],
    "icons": [
        {
            "src": "icons/icon.svg",
            "sizes": "512x512",
            "type": "image/svg+xml",
            "purpose": "any"
        },
        {
            "src": "icons/icon.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "any"
        }
    ],
    "file_handlers": [
        {
            "action": "/",
            "accept": {
                "text/*": [
                    ".txt",
                    ".text",
                    ".log",
                    ".csv",
                    ".tsv"
                ],
                "text/plain": [
                    ".txt",
                    ".text",
                    ".log",
                    ".csv",
                    ".tsv",
                    ".md",
                    ".markdown",
                    ".mdown",
                    ".mdx"
                ],
                "application/pdf": [
                    ".pdf"
                ],
                "application/json": [
                    ".json"
                ],
                "application/xml": [
                    ".xml"
                ],
                "application/javascript": [
                    ".js"
                ],
                "application/typescript": [
                    ".ts"
                ],
                "image/webp": [
                    ".webp"
                ],
                "image/svg+xml": [
                    ".svg"
                ],
                "image/jpeg": [
                    ".jpg",
                    ".jpeg"
                ],
                "image/png": [
                    ".png"
                ],
                "text/markdown": [
                    ".md",
                    ".markdown",
                    ".mdown",
                    ".mdx",
                    ".mkd",
                    ".mkdn"
                ],
                "application/markdown": [
                    ".md",
                    ".markdown",
                    ".mdown",
                    ".mdx",
                    ".mkd",
                    ".mkdn"
                ],
                "text/x-markdown": [
                    ".md",
                    ".markdown",
                    ".mdown",
                    ".mdx",
                    ".mkd",
                    ".mkdn"
                ],
                "application/x-markdown": [
                    ".md",
                    ".markdown",
                    ".mdown",
                    ".mdx",
                    ".mkd",
                    ".mkdn"
                ],
                "application/md": [
                    ".md",
                    ".markdown",
                    ".mdown",
                    ".mdx",
                    ".mkd",
                    ".mkdn"
                ],
                "text/html": [
                    ".html",
                    ".htm"
                ]
            }
        }
    ],
    "share_target": {
        "action": "/share-target",
        "method": "POST",
        "enctype": "multipart/form-data",
        "params": {
            "title": "title",
            "text": "text",
            "url": "url",
            "files": [
                {
                    "name": "mapped_files",
                    "accept": [
                        "text/plain",
                        "text/markdown",
                        "text/x-markdown",
                        "text/html",
                        "image/jpeg",
                        "image/png",
                        "image/webp",
                        "image/*",
                        "image/svg+xml",
                        "text/*",
                        "application/octet-stream",
                        "application/x-markdown",
                        "application/markdown",
                        "application/md",
                        "application/pdf",
                        "application/xml",
                        "application/javascript",
                        "application/typescript",
                        "application/json"
                    ]
                }
            ]
        }
    },
    "shortcuts": [
        {
            "name": "Viewer (MD)",
            "short_name": "viewer",
            "url": "/viewer",
            "description": "Markdown Viewer"
        },
        {
            "name": "Work Center (AI)",
            "short_name": "Analyze",
            "url": "/workcenter",
            "description": "AI-powered processing"
        }
    ],
    "handle_links": "preferred",
    "launch_handler": {
        "client_mode": "focus-existing"
    },
    "orientation": "natural",
    "edge_side": "left",
    "edge_side_delegate": {
        "url": "/",
        "edge_side": "left"
    }
}