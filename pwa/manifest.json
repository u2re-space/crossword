{
    "name": "RAS-klad",
    "short_name": "RAS-klad",
    "description": "RAS-klad",
    "start_url": "/",
    "display": "standalone",
    "background_color": "#000000",
    "theme_color": "#000000",
    "icons": [
        {
            "src": "/icon.svg",
            "sizes": "192x192",
            "type": "image/png"
        }
    ],
    "share_target": {
        "action": "/share-target",
        "method": "POST",
        "enctype": "multipart/form-data",
        "params": {
            "title": "title",
            "text": "text",
            "url": "url",
            "files": [
                { "name": "files", "accept": ["image/*", "text/*", "application/pdf"] }
            ]
        }
    }
}
