{
    "include": ["./fest/**/*.ts"],
    "exclude": ["../dist/**/*"],
    "compilerOptions": {
        "outDir": "../dist/",
        "paths": {
            "@phosphor-icons/core/assets/*": ["../node_modules/@phosphor-icons/core/assets/*"],
            "fonts/*": ["./fonts/*"],
            "fest/*": ["./fest/*"],
            "fest/cdnImport": ["./fest/cdnImport.mjs"],
            "fest/dom": ["./fest/dom/index.ts"],
            "fest/lure": ["./fest/lure/index.ts"],
            "fest/object": ["./fest/object/index.ts"],
            "fest/uniform": ["./fest/uniform/index.ts"],
            "fest/theme": ["./fest/theme/index.ts"],
            "fest/icon": ["./fest/icon/index.ts"],
            "fest/polyfill/*": ["./fest/polyfill/*"],
            "polyfill/*": ["./fest/polyfill/*"],
            "@fl-design/*": ["./fest/fl-ui/design/*"],
            "@fl-ui/*": ["./fest/fl-ui/ui/*"],
            "@fl-src/*": ["./fest/fl-ui/*"],
            "@rs-pwa/*": ["../src/pwa/*"],
            "@rs-crx/*": ["../src/crx/*"],
            "@rs-core/*": ["../src/core/*"],
            "@rs-com/*": ["../src/com/*"],
            "@rs-frontend/*": ["../src/frontend/*"],
            "@rs-shared/*": ["./*"],
            "@rs-test/*": ["../test/*"]
        },
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "lib": ["ESNext", "DOM", "WebWorker"],
        "types": ["chrome"],
        "esModuleInterop": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "allowJs": true,
        "allowArbitraryExtensions": true,
        "allowSyntheticDefaultImports": true,
        "allowUmdGlobalAccess": true,
        "allowUnreachableCode": true,
        "allowUnusedLabels": true,
        "noImplicitAny": false,
        "declaration": false,
        "noImplicitThis": false,
        "inlineSources": true,
        "inlineSourceMap": true,
        "sourceMap": false,
        "experimentalDecorators": true
    }
}
