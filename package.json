{
    "name": "crossword",
    "private": true,
    "type": "module",
    "engines": {
        "node": ">=24"
    },
    "scripts": {
        "build": "npm run build:pwa",
        "build:pwa": "vite build",
        "build:crx": "vite build --config vite.config.js --mode crx",
        "build:crx:lite": "VITE_ENABLED_VIEWS=viewer,editor,settings,history,home,print vite build --config vite.config.js --mode crx",
        "build:all": "node ./scripts/build.mjs pwa crx",
        "build:modules": "node ./scripts/build.mjs modules",
        "preview": "vite preview",
        "start": "vite",
        "dev": "vite dev",
        "test": "vite dev",
        "test:dev": "vite dev",
        "test:test": "echo 'No tests configured for crossword' && exit 0"
    },
    "browserslist": [
        "last 1 chrome version",
        "last 1 edge version"
    ],
    "overrides": {
        "@rollup/rollup-linux-x64-gnu": "npm:@rollup/wasm-node@latest",
        "rollup": "npm:@rollup/wasm-node@latest",
        "@vitejs/devtools": ">=0.0.0-alpha.32",
        "vite-plugin-pwa": ">=1.2.0"
    },
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-gnu": "npm:@rollup/wasm-node@latest",
        "@rollup/wasm-node": ">=4.57.1"
    },
    "resolutions": {
        "@rollup/rollup-linux-x64-gnu": "npm:@rollup/wasm-node@latest",
        "rollup": "npm:@rollup/wasm-node@latest"
    },
    "devDependencies": {
        "@rollup/rollup-linux-x64-gnu": "npm:@rollup/wasm-node@latest",
        "rollup": "npm:@rollup/wasm-node@latest",
        "@crxjs/vite-plugin": ">=2.2.0",
        "@jsquash/jpeg": ">=1.6.0",
        "@jsquash/png": ">=3.1.1",
        "@phosphor-icons/core": ">=2.1.1",
        "@rollup/wasm-node": ">=4.57.1",
        "@toon-format/toon": ">=2.1.0",
        "@types/sanitize-html": ">=2.14.0",
        "autoprefixer": ">=10.4.20",
        "baseline-browser-mapping": ">=2.9.19",
        "country-flag-emoji-polyfill": ">=0.1.8",
        "cssnano": ">=7.0.7",
        "culori": ">=4.0.2",
        "docx": ">=9.5.3",
        "dompurify": ">=3.2.5",
        "isomorphic-dompurify": ">=2.35.0",
