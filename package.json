        "jsox": ">=1.2.125",
        "marked": ">=15.0.12",
        "marked-katex-extension": ">=5.1.5",
        "mathml-to-latex": ">=1.5.0",
        "postcss-discard-duplicates": ">=7.0.1",
        "postcss-import": ">=16.1.0",
        "postcss-preset-env": ">=10.2.1",
